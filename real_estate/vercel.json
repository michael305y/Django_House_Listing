{
  

    "builds": [
      {
        "src": "Django_House_Listing/wsgi.py",
        "use": "@vercel/python",
        "config":	{
        "maxLambdaSize":"15mb", 
        "runtime": "python3.11"
      }
      }
    ],
  
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Django_House_Listing/wsgi.py"
      }
     ]
     
  }
  